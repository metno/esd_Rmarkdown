@article{sillmann_present_2009,
	title = {Present and future atmospheric blocking and its impact on {European} mean and extreme climate},
	volume = {36},
	copyright = {Copyright 2009 by the American Geophysical Union.},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2009GL038259/abstract},
	doi = {10.1029/2009GL038259},
	abstract = {Atmospheric blocking plays an important role in the mid-latitude climate variability and can be responsible for anomalous mean and/or extreme climate. In this study, a potential vorticity based blocking indicator is used to investigate the representation of Euro-Atlantic atmospheric blocking events in the ECHAM5/MPI-OM climate model. The impact of blocking events on present and future mean and extreme climate is studied by means of composite maps and correlation analyses. In comparison to ERA-40 re-analysis, the model represents the blocking frequency and seasonal distribution well. We show that European blocking events have a sustained influence particularly on anomalous cold winter temperatures in Europe. In a future climate, the blocking frequency is slightly diminished but the influence on the European winter climate remains robust. Due to a northeastward shift of the blocking pattern and an increase in maximum blocking duration, cold winter temperature extremes can still be expected in a future climate.},
	language = {en},
	number = {10},
	urldate = {2013-01-18},
	journal = {Geophysical Research Letters},
	author = {Sillmann, Jana and Croci-Maspoli, Mischa},
	year = {2009},
	keywords = {atmospheric blocking, climate change, Extreme events},
	pages = {n/a--n/a},
	file = {Full Text PDF:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/DA563C7T/Sillmann and Croci-Maspoli - 2009 - Present and future atmospheric blocking and its im.pdf:application/pdf;Snapshot:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/7TJCETR6/abstract.html:text/html}
}

