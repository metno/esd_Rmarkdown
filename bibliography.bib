@article{brooks_proximity_2009,
	title = {Proximity soundings for severe convection for {Europe} and the {United} {States} from reanalysis data},
	volume = {93},
	issn = {0169-8095},
	shorttitle = {4th {European} {Conference} on {Severe} {Storms} 4ECSS 4th {European} {Conference} on {Severe} {Storms}},
	url = {http://www.sciencedirect.com/science/article/pii/S0169809508002858},
	doi = {10.1016/j.atmosres.2008.10.005},
	abstract = {Proximity soundings from reanalysis data have been created for significant severe thunderstorms in the United States and Europe, along with corresponding soundings not associated with severe thunderstorms. The probability of a combination of convective available potential energy (CAPE) and deep tropospheric wind shear being associated with significant severe thunderstorms has been calculated for both areas. Probabilities of significant severe storms are higher for high CAPE and shear in Europe, but those large scale environmental conditions are experienced much more frequently in the US, so that the overall number of events is much higher in the US. Probabilities of significant storms are approximately constant for constant values of CAPE⁎Shear for each of the datasets. High values of 0–1 km wind shear and low lifted condensation levels are associated with higher probabilities that significant severe thunderstorms will be associated with significant tornadoes. A subset of the US data, taken from the southeastern US in the cool season, produces probabilities that are much closer to the European values than the overall US data. The environments also are closer to European values with relatively low CAPE and low lifted condensation levels. From the southeastern US data, it is shown that the probability of severe convection occurring with moderate values of CAPE⁎Shear is much higher in the cool season than in the warm season. It is suggested that the higher probabilities result from stronger synoptic forcing and stronger and more frequent boundaries to initiate convection.},
	number = {1–3},
	urldate = {2013-01-17},
	journal = {Atmospheric Research},
	author = {Brooks, Harold E.},
	month = jul,
	year = {2009},
	keywords = {Proximity soundings, Reanalysis data, Severe thunderstorms},
	pages = {546--553},
	file = {ScienceDirect Full Text PDF:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/AG77GVTV/Brooks - 2009 - Proximity soundings for severe convection for Euro.pdf:application/pdf;ScienceDirect Snapshot:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/45QJ4MRF/S0169809508002858.html:text/html}
