@article{kundzewicz_trend_2005,
	title = {Trend detection in river flow series: 1. {Annual} maximum flow / {Détection} de tendance dans des séries de débit fluvial: 1. {Débit} maximum annuel},
	volume = {50},
	issn = {0262-6667},
	shorttitle = {Trend detection in river flow series},
	url = {http://www.tandfonline.com/doi/abs/10.1623/hysj.2005.50.5.797},
	doi = {10.1623/hysj.2005.50.5.797},
	abstract = {Abstract Results of a study on change detection in hydrological time series of annual maximum river flow are presented. Out of more than a thousand long time series made available by the Global Runoff Data Centre (GRDC) in Koblenz, Germany, a worldwide data set consisting of 195 long series of daily mean flow records was selected, based on such criteria as length of series, currency, lack of gaps and missing values, adequate geographical distribution, and priority to smaller catchments. The analysis of annual maximum flows does not support the hypothesis of ubiquitous growth of high flows. Although 27 cases of strong, statistically significant increase were identified by the Mann-Kendall test, there are 31 decreases as well, and most (137) time series do not show any significant changes (at the 10\% level). Caution is advised in interpreting these results as flooding is a complex phenomenon, caused by a number of factors that can be associated with local, regional, and hemispheric climatic processes. Moreover, river flow has strong natural variability and exhibits long-term persistence which can confound the results of trend and significance tests.},
	number = {5},
	urldate = {2013-01-17},
	journal = {Hydrological Sciences Journal},
	author = {Kundzewicz, Zbigniew W. and Graczyk, Dariusz and Maurer, Thomas and Pińskwar, Iwona and Radziejewski, Maciej and Svensson, Cecilia and Szwed, Małgorzata},
	year = {2005},
	pages = {null--810},
	file = {Full Text PDF:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/72XPZ2TT/Kundzewicz et al. - 2005 - Trend detection in river flow series 1. Annual ma.pdf:application/pdf;Snapshot:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/7G23J8JV/hysj.2005.50.5.html:text/html}
