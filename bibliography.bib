@article{vautard_summertime_2007,
	title = {Summertime {European} heat and drought waves induced by wintertime {Mediterranean} rainfall deficit},
	volume = {34},
	copyright = {Copyright 2007 by the American Geophysical Union.},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2006GL028001/abstract},
	doi = {10.1029/2006GL028001},
	abstract = {The risk of extreme heat waves in Europe like the unprecedented one of summer 2003 is likely to increase in the future, calling for increased understanding of these phenomena. From an analysis of meteorological records over 58 years, we show that hot summers are preceded by winter rainfall deficits over Southern Europe. Subsequent drought and heat spreads northward throughout Europe in early summer, due to atmospheric transport of anomalously warm and dry air from Southern Europe in southerly wind episodes. This is shown by the observations and supported by mesoscale meteorological sensitivity simulations for Summer 1994. Moreover previous winter and early spring rainfall frequency in the Mediterranean regions is correlated with summer temperature in central continental Europe. These results emphasize the critical role of the water reservoir in the soil of continental Mediterranean areas for the maintenance of European climate.},
	language = {en},
	number = {7},
	urldate = {2013-01-18},
	journal = {Geophysical Research Letters},
	author = {Vautard, R. and Yiou, P. and D'Andrea, F. and de Noblet, N. and Viovy, N. and Cassou, C. and Polcher, J. and Ciais, P. and Kageyama, M. and Fan, Y.},
	year = {2007},
	keywords = {drought, heatwaves, summer climate},
	pages = {L07711},
	file = {Full Text PDF:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/92BEC5HT/Vautard et al. - 2007 - Summertime European heat and drought waves induced.pdf:application/pdf;Snapshot:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/JKDKM8K6/abstract.html:text/html}
}

@article{lorenz_persistence_2010,
	title = {Persistence of heat waves and its link to soil moisture memory},
	volume = {37},
	copyright = {Copyright 2010 by the American Geophysical Union.},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2010GL042764/abstract},
	doi = {10.1029/2010GL042764},
	abstract = {In this study, we assess the role of soil moisture for heat wave persistence using simulations with a regional climate model. Several studies have investigated changes in the frequency of hot summer days but very few investigated changes in their persistence. We use two different heat wave thresholds, either defined by the 90th percentile of the control run or by the 90th percentile of the respective sensitivity experiment. We identify that simulations in which soil moisture is fixed to a constant value or prescribed seasonal cycle, even with prescribed constant dry conditions, present a lower intrinsic heat wave persistence than simulations with interactive soil moisture. This effect is related to the impact of soil moisture persistence in the interactive simulations and amounts to ca. 5–10\% of the spell lengths of the 10\% hottest days in the respective simulations. Our results highlight the key role of soil moisture memory for the persistence of heat wave events, beside the known effect of soil moisture on heat wave intensity.},
	language = {en},
	number = {9},
	urldate = {2013-01-17},
	journal = {Geophysical Research Letters},
	author = {Lorenz, Ruth and Jaeger, Eric B. and Seneviratne, Sonia I.},
	year = {2010},
	keywords = {Heat waves, regional climate modeling, soil moisture memory},
	pages = {n/a--n/a}
}

