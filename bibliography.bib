@article{marsh_preliminary_2009,
	title = {Preliminary investigation into the severe thunderstorm environment of {Europe} simulated by the {Community} {Climate} {System} {Model} 3},
	volume = {93},
	issn = {0169-8095},
	shorttitle = {4th {European} {Conference} on {Severe} {Storms} 4ECSS 4th {European} {Conference} on {Severe} {Storms}},
	url = {http://www.sciencedirect.com/science/article/pii/S0169809508002573},
	doi = {10.1016/j.atmosres.2008.09.014},
	abstract = {Seasonal cycles of parameters conducive for the development of severe thunderstorms were computed using 20 years of output from the Community Climate System Model v3 (CCSM3) for both a 20th century simulation and a 21st century simulation. These parameters were compared against parameters calculated from the NCEP/NCAR Global Reanalysis data, which are of similar resolution. The CCSM3's current simulation produced seasonal and spatial distributions of both mean CAPE and favorable severe environments that were qualitatively similar to the NCEP/NCAR Global Reanalysis, although the CCSM3 underestimates the frequency of severe thunderstorm environments. Preliminary comparisons of the CCSM3's 21st century simulation under the IPCC's A2 emissions scenario to the 20th century simulation indicated a slight increase in mean CAPE in the cool season and a slight decrease in the warm season and little change in mean wind shear. However, there was a small increase in favorable severe environments for most locations resulting from an increase in the joint occurrence of high CAPE and high deep layer shear. Regions near the Mediterranean Sea experienced the biggest increase in both mean CAPE and favorable severe environments, regions near the Faeroe Islands experienced an increase in only seasonal mean CAPE, and regions across northern Europe experienced little change.},
	number = {1–3},
	urldate = {2013-01-17},
	journal = {Atmospheric Research},
	author = {Marsh, Patrick T. and Brooks, Harold E. and Karoly, David J.},
	month = jul,
	year = {2009},
	keywords = {Climate variability, Climatologies, Severe convection},
	pages = {607--618},
	file = {ScienceDirect Full Text PDF:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/MUT8GIWN/Marsh et al. - 2009 - Preliminary investigation into the severe thunders.pdf:application/pdf;ScienceDirect Snapshot:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/E3PZ25UR/S0169809508002573.html:text/html}
}

@article{dotzek_downburst-producing_2009,
	title = {Downburst-producing thunderstorms in southern {Germany}: {Radar} analysis and predictability},
	volume = {93},
	issn = {0169-8095},
	shorttitle = {4th {European} {Conference} on {Severe} {Storms} 4ECSS 4th {European} {Conference} on {Severe} {Storms}},
	url = {http://www.sciencedirect.com/science/article/pii/S0169809508002652},
	doi = {10.1016/j.atmosres.2008.09.034},
	abstract = {Three days with downburst-producing thunderstorms during the VERTIKATOR intensive observation period in June and July 2002 are studied by means of the C-band polarisation diversity radar POLDIRAD and its network of three bistatic receivers. We present the first wind vector fields from a downburst measured by such a bistatic network. The polarimetric radar data allowed testing the recent hypothesis that a dominant trigger mechanism for wet downbursts might be the cooling due to melting of small hail or graupel in the storm, and we found some evidence for this process in the VERTIKATOR storms. This could be exploited by polarimetric radar nowcasting algorithms for downburst detection. The predictability of the downburst potential was further investigated from proximity soundings and their derived indices WINDEX as well as different formulations of GUSTEX. In particular, a new formulation of GUSTEX is proposed here which shows promising predictive skill for the VERTIKATOR cases and a number of other severe (and non-severe) situations from the same region in southern Germany.},
	number = {1–3},
	urldate = {2013-01-17},
	journal = {Atmospheric Research},
	author = {Dotzek, Nikolai and Friedrich, Katja},
	month = jul,
	year = {2009},
	keywords = {Downburst, GUSTEX, Polarimetric bistatic radar, WINDEX},
	pages = {457--473},
	file = {ScienceDirect Full Text PDF:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/EC5MZ8CI/Dotzek and Friedrich - 2009 - Downburst-producing thunderstorms in southern Germ.pdf:application/pdf;ScienceDirect Snapshot:/home/rasmusb/.mozilla/firefox/1k5zjzbf.default/zotero/storage/EEHK6HN5/S0169809508002652.html:text/html}
}

